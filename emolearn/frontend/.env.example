# Frontend Environment Variables Template
# Copy this to .env and fill in your actual values
# These variables are exposed to the browser, so do NOT include secrets here

# Backend API URL
REACT_APP_API_URL=https://your-backend-api.railway.app

# WebSocket URL (must use wss:// for secure connections)
REACT_APP_WEBSOCKET_URL=wss://your-backend-api.railway.app

# Python emotion detection service
REACT_APP_PYTHON_SERVICE_URL=https://your-python-service.railway.app

# Optional: Analytics or other public API keys
REACT_APP_ENVIRONMENT=production
